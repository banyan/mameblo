<% provide(:title, "エディットできるか??????") %>
  <%= form_for(@user) do |f| %>
  <%= render 'shared/error_messages' ,object: f.object%>
  <%= f.text_field :name, placeholder: 'UserName' %>
  <%= f.text_field :email, placeholder: 'MailAddress' %>
  <%= f.password_field :password, placeholder: 'Password' %>
  <%= f.password_field :password_confirmation, placeholder: 'PasswordOnceMore !!' %>
  <%= f.submit "ぶちこめえええええ！！！！", class: "button red" %>
<% end %>
